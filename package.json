{
	"name": "dexrnwebsite",
	"private": true,
	"version": "2.1.3",
	"type": "module",
	"license": "MIT",
	"repository": "https://github.com/DexrnZacAttack/DexrnZacAttack.github.io",
	"scripts": {
		"dev": "npx tsx scripts/prebuild.ts && vite dev",
		"build": "npx tsx scripts/prebuild.ts && prettier --write . && vite build",
		"preview": "npx tsx scripts/prebuild.ts && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"postbuild": "npx svelte-sitemap --domain https://dexrnzacattack.github.io && tvkit build ./build --out ./dist --browser \"Chrome 50, Firefox 30\" --force && npx tsx scripts/postbuild.ts",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/validator": "^13.15.0",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sitemap": "^2.7.0",
		"terser": "^5.39.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.5"
	},
	"dependencies": {
		"@vitejs/plugin-legacy": "^6.1.1",
		"marked": "^15.0.9",
		"prettier-plugin-organize-imports": "^4.1.0",
		"svelte-i18n": "^4.0.1",
		"tvkit": "^0.18.0",
		"validator": "^13.15.0"
	}
}
